<wwc:wso2as-web xmlns:wwc="http://wso2as-web-config/xsd">
    <wwc:single-sign-on>true</wwc:single-sign-on>
    <wwc:statistics-publisher>
        <wwc:enabled>true</wwc:enabled>
        <wwc:stream-id>str1234</wwc:stream-id>
    </wwc:statistics-publisher>
    <wwc:classloading>
        <wwc:parent-first>false</wwc:parent-first>
        <wwc:environments>
            <wwc:environment>CXF</wwc:environment>
        </wwc:environments>
    </wwc:classloading>
    <wwc:web-service-discovery>true</wwc:web-service-discovery>
    <wwc:rest-web-services>
        <wwc:is-managed-api>true</wwc:is-managed-api>
    </wwc:rest-web-services>
</wwc:wso2as-web>